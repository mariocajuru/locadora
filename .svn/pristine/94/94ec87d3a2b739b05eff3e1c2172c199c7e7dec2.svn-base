/* Alterações Renovar */

/* ------------------------------- */
/* FIXES JSF/PRIMEFACES            */
/* ------------------------------- */

/* Remove bordas do DataGrid */
.ui-widget-content {
    border: none !important;
}

/* Define o tamanho das colunas do dataGrid s*/
.ui-datagrid-row td {
	width: 16.66666667% !important;
}

/* Corrige espaçamento errado em Grupo de botões */
.btn-group button {
	margin-right: 0px !important;
}

/* Corrige o estilo do campo datepicker */
.data .ui-inputfield {
	width: 100% !important;
	display: block !important;
}


/* Corrige o estilo do campo datepicker */
.ui-inputNum .ui-inputfield {
	width: 100% !important;
	display: block !important;
}

/* Remove Negrito */
.ui-selectmanycheckbox label {
	font-weight:normal !important;
}

/** SelectCheckbox Menu **/
.ui-selectcheckboxmenu label {
	font-weight: normal !important;
	float: left;
	text-align: left;
	margin-right: 17.5em;
	display: block;
	padding: 18px;
    width: auto;
    border: none;
    cursor: pointer;
}

.ui-selectcheckboxmenu-panel .ui-selectcheckboxmenu-list-item .ui-chkbox {
    position: absolute;
    top:50%;
   margin: 3px 0 0 3px;
      padding:3px 5px;
      padding-left: -25px;
	padding-right: -45px;
	 display: block;
}

.ui-selectcheckboxmenu-panel .ui-selectcheckboxmenu-list-item label {
	text-indent:30px;/** espaçamento primeira linha **/
	cursor: pointer;
	margin: 0;
	outline: medium none;
	z-index: 1;
	display: inline-block;
	vertical-align: top;
	border: 0 none;
	font-weight: normal;
	display: block;
	padding: 2px;
	width: 150px;
	padding-left: 15px;
	padding-right: 25px;
}



/* Espaço entre o checkbox e o label */
.ui-selectmanycheckbox label {
	padding-left: 10px;
	padding-right: 20px;
}

/* */
.ui-selectmanycheckbox .panel-body {padding-left: 5px;}

